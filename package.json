{
  "name": "meistericons",
  "version": "1.0.0",
  "description": "icons for your next project.",
  "main": "lib/index.cjs.js",
  "module": "dist/main.esm.js",
  "repository": "https://github.com/rahulrajdahal/meistericons",
  "author": "rahulrajdahal",
  "license": "MIT",
  "type": "module",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "optimize": "svgo -q -p 8 -f ./icons -o ./icons",
    "fonts": "fantasticon -n mni -p mni ./icons -o ./fonts",
    "build:icons": "node ./dist/scripts/buildIcons.js --output=./react --templateSrc=./exportTemplate.js --iconFileExtension=.ts --exportFileName=index.ts",
    "build:types": "node ./dist/scripts/buildTypes.js --output=./react",
    "build": "tsc && yarn optimize && yarn fonts && yarn build:icons"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.21",
    "fantasticon": "^1.2.3",
    "minimist": "^1.2.8",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "svgo": "^3.0.2",
    "svgson": "^5.3.1"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
