{
  "name": "meistericons",
  "version": "0.1.46",
  "description": "icons for you",
  "license": "MIT",
  "main": "./react/cjs/index.js",
  "module": "./react/esm/index.js",
  "type": "commonjs",
  "scripts": {
    "tsc": "tsc",
    "optimize": "svgo -q -p 8 -f ./icons -o ./icons ",
    "fonts": "fantasticon -n mni -p mni ./icons -o ./fonts",
    "add:iconsToTags": "node ./dist/scripts/addIconsToTags.js",
    "add:tagsToIcons": "node ./dist/scripts/addTagsToIcons.js",
    "add:iconsToCategories": "node ./dist/scripts/addIconsToCategories.js",
    "add:categoriesToIcons": "node ./dist/scripts/addCategoriesToIcons.js",
    "build:icons": "node ./dist/scripts/buildIcons.js",
    "build:icon-nodes": "node ./dist/scripts/createIconNodes.js",
    "build:types": "node ./dist/scripts/buildTypes.js",
    "build": "tsc & yarn optimize & yarn fonts & yarn add:iconsToTags & yarn add:tagsToIcons & yarn add:categoriesToIcons & yarn add:iconsToCategories & yarn build:icons & yarn build:types"
  },
  "homepage": "https://github.com/Markaa10/meistericons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Markaa10/meistericons.git"
  },
  "bugs": {
    "url": "https://github.com/Markaa10/meistericons/issues"
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "react-icon",
    "mnicons",
    "meistericons",
    "mni"
  ],
  "files": [
    "fonts/",
    "icons/",
    "react/",
    "icon-nodes.json",
    "tags.json",
    "categories.json"
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.16.0",
    "@svgr/cli": "^5.5.0",
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^6.1.2",
    "@svgr/plugin-prettier": "^6.1.2",
    "@types/babel-core": "^6.25.7",
    "@types/minimist": "^1.2.2",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.15",
    "@types/terser": "^3.12.0",
    "camelcase": "^7.0.1",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0",
    "svgson": "^5.3.1",
    "terser": "^5.19.2"
  },
  "dependencies": {
    "@babel/core": "^7.22.9",
    "minimist": "^1.2.8"
  }
}
