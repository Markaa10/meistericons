{
  "name": "meistericons",
  "module": "index.ts",
  "type": "module",
  "version": "0.1.51",
  "homepage": "https://github.com/rahulrajdahal/meistericons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulrajdahal/meistericons.git"
  },
  "bugs": {
    "url": "https://github.com/rahulrajdahal/meistericons/issues"
  },
  "keywords": [
    "icons",
    "svg",
    "mnicons",
    "meistericons",
    "mni"
  ],
  "files": [
    "fonts/",
    "icon-nodes.json",
    "tags.json",
    "categories.json"
  ],
  "scripts": {
    "fonts": "fantasticon -n mni -p mni icons -o fonts",
    "optimize": "svgo -f ./icons -o ./icons",
    "build:icons": "bun run ./scripts/buildIcons.ts",
    "build:types": "bun run ./scripts/buildTypes.ts",
    "build": "bun build:icons && bun build:types"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fantasticon": "^2.0.0",
    "rollup": "^3.29.2",
    "svgo": "^3.0.2",
    "svgson": "^5.3.1",
    "vue": "^3.3.4"
  }
}
