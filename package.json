{
  "name": "meistericons",
  "version": "0.0.1",
  "description": "icon library for you",
  "main": "index.ts",
  "repository": {
    "url": "https://github.com/rahulrajdahal/meistericons.git",
    "type": "git"
  },
  "scripts": {
    "build:icons-nodes": "pnpm --filter build-icons build:icon-nodes",
    "build:icons-react": "pnpm --filter react build",
    "build:icons-vue": "pnpm --filter vue build",
    "build:icons-vue-latest": "pnpm --filter vue-latest build",
    "build": "pnpm run build:icons-nodes && pnpm run build:icons-react && pnpm run build:icons-vue && pnpm run build:icons-vue-latest"
  },
  "keywords": [
    "meistericons",
    "mni",
    "icons",
    "icon-library",
    "vector-icons",
    "svg-icons",
    "react-icons",
    "vue-icons"
  ],
  "author": "Rahul Raj Dahal",
  "license": "MIT"
}