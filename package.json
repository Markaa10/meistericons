{
  "name": "meistericons",
  "module": "index.ts",
  "type": "module",
  "version": "0.1.54",
  "homepage": "https://meistericons.com/how-to-use",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulrajdahal/meistericons.git"
  },
  "bugs": {
    "url": "https://github.com/rahulrajdahal/meistericons/issues"
  },
  "keywords": [
    "icons",
    "svg",
    "mnicons",
    "meistericons",
    "mni"
  ],
  "files": [
    "fonts/",
    "icon-nodes.json",
    "tags.json",
    "categories.json"
  ],
  "scripts": {
    "fonts": "fantasticon -n mni -p mni icons -o fonts",
    "optimize": "svgo -f ./icons -o ./icons",
    "build:icons": "bun run ./scripts/buildIcons.ts",
    "build:icons-react": "bun run ./scripts/buildIcons.ts --outdir=./packages/react/icons --ext=js",
    "add:tags": "bun run ./scripts/addTags.ts",
    "add:categories": "bun run ./scripts/addCategories.ts",
    "build:types": "bun run ./scripts/buildTypes.ts",
    "build": "bun fonts && bun optimize && build:icons && bun add:tags && bun add:categories && bun build:types"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.9.0",
    "bun-types": "latest",
    "minimist": "^1.2.8",
    "typescript": "^5.2.2",
    "vitepress": "^1.0.0-rc.31"
  },
  "dependencies": {
    "fantasticon": "^2.0.0",
    "rollup": "^3.29.2",
    "svgo": "^3.0.2",
    "svgson": "^5.3.1",
    "vue": "^3.3.4"
  }
}
