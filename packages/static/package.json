{
  "name": "@mni/static",
  "version": "0.0.1",
  "description": "static assets for meistericons",
  "main": "./lib/cjs/mni-static.cjs.min.cjs",
  "main:umd": "./lib/umd/mni-static.umd.min.js",
  "module": "./lib/es/mni-static.es.min.js",
  "bugs": {
    "url": "https://github.com/rahulrajdahal/meistericons/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "url": "https://github.com/rahulrajdahal/meistericons.git",
    "type": "git",
    "directory": "/packages/static"
  },
  "scripts": {
    "fonts": "bun run ./src/generateFonts.ts && fantasticon -n mni -p mni ./fontIcons -o ./fonts && rm -r ./fontIcons",
    "bundle": "rollup -c",
    "build": "bun run ./src/build.ts"
  },
  "dependencies": {
    "@mni/build-tools": "workspace:*"
  },
  "files": [
    "icons",
    "fonts",
    "icon-nodes.json"
  ],
  "keywords": [
    "meistericons",
    "mni",
    "icons",
    "icon-library",
    "vector-icons",
    "svg-icons",
    "mni-fonts",
    "font-icons",
    "web-fonts",
    "mni-styles",
    "mni-css"
  ],
  "author": "Rahul Raj Dahal",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.11.25",
    "rollup": "^4.12.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "fantasticon": "^2.0.0",
    "rollup-plugin-visualizer": "^5.12.0"
  },
  "exports": {
    "default": "./lib/umd/static.umd.min.js",
    "import": "./lib/es/static.es.min.mjs",
    "require": "./lib/cjs/static.cjs.cjs"
  }
}