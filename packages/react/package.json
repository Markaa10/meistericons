{
  "name": "mni-react",
  "version": "0.0.1",
  "description": "React icon library for meistericons.",
  "main": "./lib/cjs/mni-react.cjs.min.cjs",
  "main:umd": "./lib/umd/mni-react.umd.min.js",
  "module": "./lib/es/mni-react.es.min.js",
  "repository": {
    "url": "https://github.com/rahulrajdahal/meistericons.git",
    "type": "git",
    "directory": "/packages/react"
  },
  "bugs": {
    "url": "https://github.com/rahulrajdahal/meistericons/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:icons": "rm -r ./icons && build-icons --pkg=../react",
    "bundle": "rollup -c",
    "build": "bun run build:icons && bun run bundle"
  },
  "dependencies": {
    "mni-build-icons": "workspace:*",
    "mni-build-tools": "workspace:*"
  },
  "keywords": [
    "meistericons",
    "mni",
    "icons",
    "icon-library",
    "vector-icons",
    "svg-icons",
    "react-icons"
  ],
  "author": "Rahul Raj Dahal",
  "license": "ISC",
  "exports": {
    "import": "./lib/es/mni-react.es.min.mjs",
    "require": "./lib/cjs/mni-react.cjs.min.cjs"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/react": "^18.2.64",
    "rollup": "^4.12.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-visualizer": "^5.12.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}